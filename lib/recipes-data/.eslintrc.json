{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "parserOptions": {
        "project": [
          "lib/recipes-data/tsconfig.lib.json"
        ]
      },
      "rules": {}
    },
    {
      "files": [
        "*.test.ts",
        "*.test.tsx"
      ],
      "parserOptions": {
        "project": [
          "lib/recipes-data/tsconfig.spec.json"
        ]
      },
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    }
  ]
}
